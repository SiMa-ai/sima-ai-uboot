/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include "simaai-davinci-h.dtsi"


/ {
	model = "SiMa.ai DaVinci Half-Height-Half-Length Board";

	chosen {
		bootargs = "console=ttyS0,115200n8 root=/dev/ram0 rw rootfstype=ramfs";
	};

	aliases {
		ethernet0 = &eth0;
		ethernet1 = &eth1;
		mmc0 = &emmc;
		mmc1 = &sd;
		console   = &uart0;
	};
    
	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x0 0x80000000>;
	};

	memory@180000000 {
		device_type = "memory";
		reg = <0x1 0x80000000 0x0 0x80000000>;
	};
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&emmc {
	status = "okay";
};

&sd {
	status = "okay";
};

&eth0 {
    snps,reset-gpio = <&porta 4 GPIO_ACTIVE_LOW>;
    snps,reset-delays-us = <0 20000 10000>;
	status = "okay";
};

&eth1 {
    snps,reset-gpio = <&porta 5 GPIO_ACTIVE_LOW>;
    snps,reset-delays-us = <0 20000 10000>;
	status = "okay";
};

&pcie_ep {
	status = "okay";
};
